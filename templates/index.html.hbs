<html>
  <head>
    <title>{{title}}</title>
    <meta property="og:title" content="{{title}}" />
    <meta property="og:url" content="{{redirectUrl}}" />
    <meta property="og:image" content="{{photoUrl}}" />
    <meta property="og:description" content="{{description}}" />
    <meta property="og:image:alt" content="{{description}}" />
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:title" content="{{title}}" />
    <meta name="twitter:description" content="{{description}}" />
    <meta name="twitter:image" content="{{photoUrl}}" />
    <link rel="canonical" href="{{redirectUrl}}" />
    <link rel="image_src" href="{{photoUrl}}" />
  </head>
  <body>
    <h1 style="display:none;">{{title}}</h1>
    <p style="display:none;">{{description}}</p>
    <img src="{{photoUrl}}" style="display:none;" />
    <a style="display:none;" href="{{redirectUrl}}">{{redirectUrl}}</a>
  </body>
  <script>
    window.location.replace("{{redirectUrl}}");   
  </script>
  <noscript>I am sorry that your browser doesn't support JavaScript. Please <a href="{{redirectUrl}}">click here</a> to go to your destination</noscript>
</html>